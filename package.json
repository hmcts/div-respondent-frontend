{
  "name": "divorce-respondent-frontend",
  "description": "Divorce Respondent Frontend",
  "version": "0.0.1",
  "license": "MIT",
  "engines": {
    "node": ">=12.13.0"
  },
  "jest": {
    "verbose": true,
    "testURL": "https://localhost:3000"
  },
  "scripts": {
    "start": "NODE_PATH=. node server.js",
    "setup": "NODE_PATH=.",
    "dev": "NODE_ENV=dev REDIS_URL=redis://127.0.0.1:6379 NODE_PATH=. node-dev server.js",
    "dev-debug": "NODE_ENV=dev NODE_PATH=. node-dev --inspect=0.0.0.0:9230 server.js",
    "mocks": "NODE_ENV=dev NODE_PATH=. node-dev mocks/index.js",
    "test": "NODE_PATH=. NODE_ENV=testing LOG_LEVEL=OFF nyc mocha --opts 'test/unit/mocha.opts' --exit 'test/unit/**/*.test.js' --timeout 15000",
    "test:all": "yarn lint; yarn test:unit; yarn test:a11y; yarn test:validation;",
    "test:unit": "NODE_PATH=. NODE_ENV=testing LOG_LEVEL=OFF mocha --opts 'test/unit/mocha.opts' --exit 'test/unit/**/*.test.js' --timeout 15000",
    "test:mocks": "NODE_PATH=. LOG_LEVEL=ERROR codeceptjs run -c test/mocks/codecept.mocks.js --steps",
    "test:smoke": "NODE_PATH=. LOG_LEVEL=ERROR codeceptjs run -c test/e2e/codecept.smoke.conf.js --steps",
    "test:coverage": "nyc check-coverage --statements 80 --branches 80 --functions 80 --lines 80",
    "test:a11y": "NODE_ENV=testing NODE_PATH=. LOG_LEVEL=ERROR mocha --exit test/a11y --reporter spec --recursive --timeout 15000 --exit --reporter mochawesome --reporter-options reportDir=functional-output,reportFilename=a11y",
    "sonar-scan": "node_modules/sonar-scanner/bin/sonar-scanner",
    "test:functional": "NODE_PATH=. LOG_LEVEL=DEBUG codeceptjs run-multiple parallel -c test/e2e/codecept.conf.js --steps --reporter mocha-multi",
    "test:fullfunctional": "NODE_PATH=. NODE_ENV=ci LOG_LEVEL=DEBUG codeceptjs run-multiple parallel -c test/e2e/codecept.conf.js --steps --reporter mocha-multi",
    "fortifyScan": "./test/java/gradlew -p test/java fortifyScan",
    "test:validation": "NODE_PATH=. NODE_ENV=testing LOG_LEVEL=ERROR mocha --exit test/w3cjs --timeout 30000",
    "test:crossbrowser": "./bin/run-crossbrowser-tests.sh",
    "test-crossbrowser-e2e": "NODE_PATH=. NODE_ENV=ci LOG_LEVEL=DEBUG node_modules/.bin/codeceptjs run-multiple  ${BROWSER_GROUP:-'--all'} -c test/e2e/saucelabs.conf.js --steps --grep @cross-browser-test --reporter mocha-multi",
    "lint": "eslint ."
  },
  "pre-commit": [
    "lint"
  ],
  "dependencies": {
    "@hmcts/ctsc-web-chat": "^0.3.11",
    "@hmcts/div-document-express-handler": "^1.1.0",
    "@hmcts/div-idam-express-middleware": "^6.6.2",
    "@hmcts/look-and-feel": "4.0.9",
    "@hmcts/nodejs-healthcheck": "^1.7.0",
    "@hmcts/nodejs-logging": "^3.0.1",
    "@hmcts/one-per-page": "^5.4.0",
    "@hmcts/properties-volume": "^0.0.13",
    "applicationinsights": "1.7.0",
