    "postcss": "^8.2.15",
    "serialize-javascript": ">=2.1.1",
    "ssri": ">=6.0.2 <7.0.0 || >=8.0.1",
    "yargs-parser": ">=13.1.2 <14.0.0 || >=18.1.2",
    "y18n": ">=5.0.5||>=4.0.1 <5.0.0||>=3.2.2 <4.0.0",
    "trim-newlines": "^4.0.1",
    "glob-parent": "^5.1.2",
    "tar": ">=4.4.18 <5.0.0 || >=5.0.10 <6.0.0 || >=6.1.9",
    "path-parse": ">=1.0.7",
    "url-parse": ">=1.5.2",
    "set-value": "^4.0.1",
    "ansi-regex": "^5.0.1",
    "acorn": "^5.7.4",
    "normalize-url": "^4.5.1",
    "ws": "^5.2.3"
  },
  "nyc": {
    "report-dir": "coverage",
    "temp-directory": "./coverage/.nyc_output",
    "reporter": [
      "lcov",
      "text"
    ],
    "exclude": [
      "test/**/*",
      "mocks/**/*"
    ]
  }
}
