    "cookies": "^0.8.0",
    "express": "^4.17.1",
    "express-limiter": "^1.6.1",
    "glob": "^7.1.6",
    "handlebars": ">=4.3.0",
    "helmet": "^3.12.2",
    "hpkp": "^2.0.0",
    "html-entities": "^1.2.1",
    "ioredis": "^4.28.2",
    "joi": "^14.3.1",
    "launchdarkly-node-server-sdk": "^5.13.1",
    "lodash": "^4.17.11",
    "marked": ">=4.0.10",
    "moment": "^2.24.0",
    "os": "^0.1.1",
    "paths": "^0.1.1",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.8",
    "serialize-javascript": ">=2.1.1",
    "tar": ">=4.4.18",
    "uuid": "^8.1.0",
    "y18n": ">=5.0.5||>=4.0.1 <5.0.0||>=3.2.2 <4.0.0",
    "yargs-parser": ">=13.1.2 <14.0.0 || >=18.1.2"
  },
